{
  "name": "Advanced Password Manager",
  "description": "Advanced password manager, which manages password with the security of you!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "./background/background.js"
  },
  "permissions": ["storage", "webNavigation", "scripting"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "action": {
    "default_popup": "extension/extensionwindow.html",
    "default_icon": {
      "128": "assets/logo.png"
    }
  },
  "icons": {
    "128": "assets/logo.png"
  }
}
